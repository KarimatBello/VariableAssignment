package com.company;


public class Main {

	public static void main(String[] args) {
	int x;
	x = 50;
	System.out.println("Test Score initialized:" + x);
	int y;
	y = (int) 24.99;
	System.out.println("Price tag:" + y);
	String firstname = new String("Bobby");
	String lastname = new String("Johnson");
	System.out.println("Username:" + firstname + lastname);



    }
}
